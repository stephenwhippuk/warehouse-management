  "owner": "warehouse-service",
  "status": "fulfilled",
  "fields": [
    {
      "name": "id",
      "classification": "identity",
      "status": "provided",
      "method": "direct",
      "dtoMappings": ["WarehouseItemDto", "WarehouseListDto"],
      "security": {
        "access": "public",
        "encrypt": false
      },
      "description": "Unique warehouse identifier"
    },
    {
      "name": "code",
      "classification": "identity",
      "status": "provided",
      "method": "direct",
      "dtoMappings": ["WarehouseItemDto", "WarehouseListDto"],
      "security": {
        "access": "public",
        "encrypt": false
      },
      "description": "Warehouse code (e.g., WH-001)"
    },
    {
      "name": "name",
      "classification": "core",
      "status": "provided",
      "method": "direct",
      "dtoMappings": ["WarehouseItemDto", "WarehouseListDto"],
      "security": {
        "access": "public",
        "encrypt": false
      },
      "description": "Warehouse name"
    },
    {
      "name": "status",
      "classification": "core",
      "status": "provided",
      "method": "direct",
      "dtoMappings": ["WarehouseItemDto", "WarehouseListDto"],
      "security": {
        "access": "public",
        "encrypt": false
      },
      "description": "Warehouse operational status (active, inactive, maintenance)"
    }
