{
  "name": "DeallocateInventory",
  "version": "1.0",
  "uri": "/api/v1/inventory/{id}/deallocate",
  "method": "POST",
  "basis": "InventoryManagementService.DeallocateStock",
  "authentication": "ApiKey",
  "description": "Deallocate previously allocated inventory",
  "parameters": [
    {
      "name": "id",
      "location": "Route",
      "type": "UUID",
      "required": true,
      "description": "Inventory record ID"
    },
    {
      "name": "request",
      "location": "Body",
      "type": "DeallocateInventoryRequest",
      "required": true,
      "description": "Deallocation details"
    }
  ],
  "responses": [
    {
      "status": 200,
      "type": "InventoryOperationResultDto",
      "description": "Deallocation successful"
    },
    {
      "status": 400,
      "type": "ErrorDto",
      "description": "Invalid request parameters"
    },
    {
      "status": 404,
      "type": "ErrorDto",
      "description": "Inventory record not found"
    },
    {
      "status": 409,
      "type": "ErrorDto",
      "description": "Cannot deallocate more than allocated quantity"
    },
    {
      "status": 401,
      "type": "ErrorDto",
      "description": "Unauthorized"
    },
    {
      "status": 500,
      "type": "ErrorDto",
      "description": "Internal server error"
    }
  ]
}
